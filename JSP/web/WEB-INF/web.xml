<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>menu_jsp</servlet-name>
        <servlet-class>jsp.menu_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>jsp.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usuario</servlet-name>
        <servlet-class>jsp.usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Menu_Banco_Dados_jsp</servlet-name>
        <servlet-class>jsp.Menu_Banco_Dados_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Redireciona_jsp</servlet-name>
        <servlet-class>jsp.Redireciona_jsp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>menu_jsp</servlet-name>
        <url-pattern>/menu_jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usuario</servlet-name>
        <url-pattern>/usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Menu_Banco_Dados_jsp</servlet-name>
        <url-pattern>/Menu_Banco_Dados_jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Redireciona_jsp</servlet-name>
        <url-pattern>/Redireciona_jsp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
